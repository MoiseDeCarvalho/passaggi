 <div class="row">
	  <h4 class="center">Risultato della ricerca</h4>

	  <br/>
	  <div class="msg-error"><span id="result_message"></span></div>
	  <% if !@feedbacks_founded.nil? %>
	    <table class="striped highlight centered"> 
	    
	      <thead>
	        <tr>
	        	<th>ID</th>
	          	<th>Punteggio</th>
	          	<th colspan="3"></th>
	        </tr>
	      </thead>
	 
	       <tbody>
	        <% @feedbacks_founded.each do |feedback| %>
			          <tr>
			          	<td><%= feedback.id %>
			            <td><%=feedback.score %></td>
			          </tr>
	      </tbody>
	  
	    </table>
	</div>
</div>