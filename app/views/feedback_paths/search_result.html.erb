<div class="row">
	  <h4 class="center">Risultato della ricerca</h4>

	  <br/>
	  <div class="msg-error"><span id="result_message"></span></div>
	  <% if !@feedback_paths_founded.nil? %>
	    <table class="striped highlight centered"> 
	    
	      <thead>
	        <tr>
	        	<th>Id del path</th>
	          	<th>Id dell'utente</th>
	          	<th colspan="3"></th>
	        </tr>
	      </thead>
	 
	       <tbody>
	        <% @feedback_paths_founded.each do |feedback_path| %>
			          <tr>
			          	<td><%= feedback_path.path_offer_id %>
			            <td><%= feedback_path.user_id %></td>
			          </tr>
	      </tbody>
	    </table>
	</div>