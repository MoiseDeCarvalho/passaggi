<div class="row">
	<div class="col l9 offset-l1">
		<%= form_tag search_result_path_offers_path, :method => 'get' do %>
		<div class="col l3 field">
			<%= form.label :user_id, class:"label-custom" %>
			<%= select_tag :user_id, options_from_collection_for_select(@users 'id', 'id'), id: 'ty' %>
	    </div>

	    <div class="col l3 field">
	        <%= form.label :path_offers_id, class:"label-custom" %>
	        <%= select_tag :path_offers_id, options_from_collection_for_select(@path_offers, 'id', 'id'), id: 'ty' %>
	    </div>
       	<br/>
        <div class="actions">
		      <%= submit_tag "Cerca" , class:"waves-effect waves-light btn btn-devise" %>
	    </div>
      <% end %>
	</div>
</div>