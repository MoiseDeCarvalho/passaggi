<p id="notice"><%= notice %></p>

<h1>Feedback Paths</h1>
<% if !@feedback_path.nil? %>    
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Path offers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback_paths.each do |feedback_path| %>
      <tr>
        <td><%= feedback_path.user_id %></td>
        <td><%= feedback_path.path_offers_id %></td>
        <td><%= link_to 'Show', feedback_path %></td>
        <td><%= link_to 'Edit', edit_feedback_path_path(feedback_path) %></td>
        <td><%= link_to 'Destroy', feedback_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% end %>
<br>

<%= link_to 'New Feedback Path', new_feedback_path_path %>
