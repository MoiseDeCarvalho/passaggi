<div class="row">
	  <h4 class="center">Risultato della ricerca</h4>

	  <br/>
	  <div class="msg-error"><span id="result_message"></span></div>
	  <% if !@profiles_founded.nil? %>
	    <table class="striped highlight centered"> 
	    
	      <thead>
	        <tr>
	        	<th>ID</th>
	          	<th>Nome</th>
	          	<th>Cognome</th>
	          	<th>Indirizzo</th>
	          	<th>Data di nascita</th>
	          	<th>Numero di cellulare</th>
	          	<th>Citta</th>
	          	<th colspan="3"></th>
	        </tr>
	      </thead>
	 
	       <tbody>
	        <% @profiles_founded.each do |profile| %>
			          <tr>
			          	<td><%= profile.id %>
			            <td><%= profile.name %></td>
			            <td><%= profile.surname %></td>
			            <td><%= profile.address %></td>
			            <td><%= profile.data_di_nascita %></td>
			            <td><%= profile.mobile %></td>
			            <td><%= profile.city %></td>
			          </tr>
	      </tbody>
	  
	    </table>
	</div>
</div>